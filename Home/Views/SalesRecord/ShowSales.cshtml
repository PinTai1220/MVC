@model IEnumerable<Home.Models.SalesRecord>
@{
    ViewBag.Title = "ShowSales";
    Layout = "~/Views/Shared/ParentView.cshtml";
    List<object> sourse = ViewBag.att;
}

<link href="~/Scripts/layui-v2.4.5/layui/css/layui.css" rel="stylesheet" />
<script src="~/Scripts/layui-v2.4.5/layui/layui.js"></script>

<table class="layui-table">
    <colgroup>
        <col width="150">
        <col width="200">
        <col>
    </colgroup>
    <thead>
        <tr>
            <th>编号</th>
            <th>用户名称</th>
            <th>商品名称</th>
            <th>订单号</th>
            <th>商品单价</th>
            <th>购买数量</th>
            <th>金额</th>
            <th>状态</th>
            <th>创建时间</th>
        </tr>
    </thead>
    @foreach (dynamic item in sourse)
    {
        <tr>
            <td>@item.SalesRecordId</td>
            <td>@item.UserName</td>
            <td>@item.GoodName</td>
            <td>@item.OrderNum</td>
            <td>@item.GoodPrice</td>
            <td>@item.Num</td>
            <td>@item.SumPrice</td>
            <td>@item.SType</td>
            <td>@item.SCreateTime</td>
        </tr>
    }
</table>